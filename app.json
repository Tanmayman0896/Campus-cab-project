{
  "expo": {
    "name": "Carpool App",
    "slug": "carpool-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "main": "index.js",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "webpack"
    },
    "plugins": [],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
